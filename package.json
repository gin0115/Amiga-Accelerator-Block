{
    "name": "gin0115-amiga-accelerator-blocs",
    "version": "0.1.0",
    "description": "Block for assigning meta data about Amiga Accelerator cards to a custom post type.",
    "author": "Glynn Quelch",
    "license": "GPL-2.0-or-later",
    "main": "build/index.js",
    "scripts": {
        "all": "npm run build && npm run editor-css",
        "build": "wp-scripts build",
        "editor-css": "node-sass --output-style compressed --include-path scss src/editor.scss build/editor.css",
        "front-css": "node-sass --output-style compressed --include-path scss src/style.scss build/style.css",
        "format:js": "wp-scripts format-js",
        "lint:css": "wp-scripts lint-style",
        "lint:js": "wp-scripts lint-js",
        "start": "wp-scripts start",
        "packages-update": "wp-scripts packages-update"
    },
    "dependencies": {
        "@wordpress/block-editor": "^7.0.1",
        "@wordpress/blocks": "^11.0.1",
        "@wordpress/i18n": "^4.2.1",
        "@wordpress/scripts": "^18.1.0",
        "mini-css-extract-plugin": "^2.4.5",
        "node-sass": "^7.0.0"
    },
    "devDependencies": {
        "copy-webpack-plugin": "^6.4.1",
        "css-minimizer-webpack-plugin": "^3.3.0",
        "sass-loader": "^12.4.0",
        "throttle-debounce": "^3.0.1"
    }
}